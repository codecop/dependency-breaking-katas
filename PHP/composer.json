{
    "name": "codecop/dependency-breaking-katas",
    "description": "Dependency Breaking Katas",
    "require": {
        "php": ">=7.3.0",
        "ext-pdo": "*",
        "brick/math": "0.9.*",
        "mashape/unirest-php": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.*",
        "mockery/mockery": "1.6.*"
    },
    "autoload": {
        "psr-4": {
            "Org\\Codecop\\Dependencies\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Org\\Codecop\\Dependencies\\Tests\\": "test\/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "tests": "phpunit"
    }
}